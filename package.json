{
  "name": "tag-this-app",
  "version": "1.0.0",
  "description": "MERN inventory management with passport authentication",
  "main": "server.js",
  "proxy": "http://localhost:3001",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Arthur Doelp, Ben Leone, Tim Look, Meg Rydzewski",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.9.0"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  }
}
